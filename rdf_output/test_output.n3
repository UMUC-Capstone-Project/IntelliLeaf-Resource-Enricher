              FMA:FMA_84050 , dcr:dctheradir_341 , IMR:IMR_0000025 , CL:CL_0000084 , CL:CL_000576 , dcr:dctheradir_356 , CL:CL_0000451 , ncbitax:NCBITaxon_9606 ;
