    <dc:creator>Chatsudthipong, Varanuj</dc:creator>
    <dc:identifier>http://www.ncbi.nlm.nih.gov/pubmed/22316332</dc:identifier>
    <dc:creator>Yimlamai, Tossaporn</dc:creator>
  </rdf:Description>
