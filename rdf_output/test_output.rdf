    <rdfs:label>Activation of DC by transduction with lentiviral vectors</rdfs:label>
    <dcr:hasStimulusType rdf:resource="http://purl.org/obo/owl/NCBITaxon#NCBITaxon_11646"/>
    <dcr:hasMoleculeType rdf:resource="http://purl.org/obo/owl/IMR#IMR_0000025"/>
