    <dc:title>Response of adipose tissue to early infection with Trypanosoma cruzi (Brazil strain).</dc:title>
    <dc:creator>Nagajyothi, Fnu</dc:creator>
    <dc:creator>Zhao, Dazhi</dc:creator>
    <dc:date>2012</dc:date>
