  <rdf:Description rdf:about="http://dc-research.eu/rdf/dataset/84">
    <rdfs:label>Studies of responses to viral infection in TLR K/O mice</rdfs:label>
    <dcr:hasExperimentalDesignType rdf:resource="http://mged.sourceforge.net/ontologies/MGEDOntology.owl#MO_454"/>
    <dcr:usesBioMat rdf:resource="http://dc-research.eu/rdf/biomaterial/56"/>
