  <rdf:Description rdf:about="http://dc-research.eu#document/22083669">
    <dc:creator>Tsukamoto, Yoshitane</dc:creator>
    <dc:creator>Yamamura, Masahiro</dc:creator>
    <dc:creator>Nakai, Mayumi</dc:creator>
