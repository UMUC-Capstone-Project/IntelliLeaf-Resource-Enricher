  <rdf:Description rdf:about="http://dc-research.eu#document/22445076">
    <dc:source>Immunobiology</dc:source>
    <dc:creator>Boissier, Marie-Christophe</dc:creator>
    <dc:creator>Singh-Jasuja, Harpreet</dc:creator>
    <dc:identifier>http://www.ncbi.nlm.nih.gov/pubmed/22445076</dc:identifier>
