    <rdfs:label>Standardized method for the immuno-monitoring of DC vaccination trials</rdfs:label>
    <dcr:usesBioMat rdf:resource="http://dc-research.eu/rdf/biomaterial/403"/>
    <dcr:hasBioSourceType rdf:resource="http://purl.org/obo/owl/DC_CL#DC_CL_0000103"/>
