  <rdf:Description rdf:about="http://dc-research.eu/rdf/tool/5">
    <rdfs:label>SILAC (Stable Isotope Labelling by Amino Acids in Cell Culture) </rdfs:label>
    <dcr:hasKnowledgeable rdf:resource="http://dc-research.eu/rdf/person/96"/>
