    <rdfs:label>Role of GM-CSF in human DC differentiation</rdfs:label>
    <dcr:has_document rdf:resource="http://dc-research.eu/rdf/document/115"/>
    <rdf:type rdf:resource="http://purl.obofoundry.org/obo/IAO_0000100"/>
    <dcr:hasMoleculeType rdf:resource="http://purl.org/obo/owl/IMR#IMR_0100390"/>
