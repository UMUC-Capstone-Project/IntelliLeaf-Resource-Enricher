    <rdfs:label>Clinical trials of B-CLL patients treated with Apo-DC +/- GM-CSF in progress</rdfs:label>
    <dcr:hasMoleculeType rdf:resource="http://purl.org/obo/owl/IMR#IMR_0000018"/>
