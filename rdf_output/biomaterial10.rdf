    <rdf:type rdf:resource="http://dc-research.eu#dctheradir_5"/>
    <dcr:hasCellLineType rdf:resource="http://purl.org/obo/owl/BTO#BTO_0000664"/>
    <dcr:usesBioMat rdf:resource="http://dc-research.eu/rdf/biomaterial/531"/>
