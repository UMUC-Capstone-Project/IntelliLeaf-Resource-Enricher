    <dcr:hasMoleculeType rdf:resource="http://purl.org/obo/owl/IMR#IMR_0001789"/>
    <rdfs:label>Definition of novel innate signalling pathway in DC via dectin-1</rdfs:label>
    <dcr:hasStimulusType rdf:resource="http://purl.org/obo/owl/IMR#IMR_0001789"/>
    <dcr:usesBioMat rdf:resource="http://dc-research.eu/rdf/biomaterial/300"/>
    <dcr:hasMoleculeType rdf:resource="http://purl.org/obo/owl/IMR#IMR_0700564"/>
