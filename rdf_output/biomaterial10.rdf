    <rdfs:label>Study of functional homogeneity by transcriptional screening of subsets of CD8 PBL </rdfs:label>
    <rdf:type rdf:resource="http://dc-research.eu#dctheradir_1"/>
    <dcr:has_document rdf:resource="http://dc-research.eu/rdf/document/70"/>
